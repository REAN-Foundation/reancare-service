meta {
  name: Get available care plans
  type: http
  seq: 4
}

get {
  url: {{BASE_URL}}/care-plans?provider=AHA
  body: json
  auth: none
}

params:query {
  provider: AHA
}

headers {
  Authorization: Bearer {{PATIENT_USER_JWT}}
  x-api-key: {{API_CLIENT_INTERNAL_KEY}}
  Content-Type: application/json
}

tests {
  
  test("Request is successfull", function () {
      expect(res.getStatus()).to.equal(200);
      var jsonRes = res.getBody();
      expect(jsonRes.Status).to.eql('success');
  });
  
  test("List of available care plans is returned", function () {
  
      var jsonRes = res.getBody();
      expect(jsonRes.Data).to.have.property('AvailablePlans');
      expect(jsonRes.Data.AvailablePlans.length).greaterThan(0);
  
  });
  
}
